<flight> = <departure> <arrival> {vf.fly($1, $2)};
[flight] = <arrival> <departure> {vf.fly($2, $1)};

<departure> = <from> <city> {nf.I($2)};

<arrival> = <to> <city> {nf.I($2)};
[arrival] = <arrival> {nf.arrival($1)};

<from> = "从" ;

<to> = "到" | "去" | "飞"; 

<city> = "北京" {nf.I("BeiJing")}
       | "天津" {nf.I("TianJin")}
       | "上海" {nf.I("ShangHai")}
       ;

